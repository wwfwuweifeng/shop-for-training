  <insert id="insertOrders">
    insert into sft_order(id,order_id,cart_num,buyer_id,shop_id,shop_name,order_total_money,
    state,pay_id,create_time,pay_time,send_time,deal_time,receiver_people,receiver_address,express_num,update_time) values
    <!--item就是List里每一项的对象名，要用","分割每一条数据，最后要";"结尾-->
